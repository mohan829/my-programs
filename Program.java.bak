class Program 
{
	public static void main(String[] args) 

	{
		int n= 4;
		for(int i=1;i<=n;i++);{
			for(int j=1;j<=1;j++){
				System.out.print(j+" ");
			}
			for(int k=1;k<=2*(n-1);k++){
				System.out.print(" ");
			}
			for(int j=1;j>=1;j--){
				System.out.print(j+" ");
			}
			System.out.println(" ");
		}
	}
}
